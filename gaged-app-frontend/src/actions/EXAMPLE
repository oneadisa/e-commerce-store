                  (openTab === 1 ? "border-Dark-blue" : "")
                }
                onClick={(e) => {
                  e.preventDefault();
                  setOpenTab(1);
                }}
                data-toggle="tab"
                href="#link1"
                role="tablist"
              >
                Overview
              </a>
            </li>
            <li className="-mb-px mr-2 last:mr-0 flex-auto text-center">
              <a
                className={
                  "text-lg font-semibold border-b-4  px-2 py-1" +
                  (openTab === 2 ? "border-Dark-blue" : "")
                }
                onClick={(e) => {
                  e.preventDefault();
                  setOpenTab(2);
                }}
                data-toggle="tab"
                href="#link2"
                role="tablist"
              >
                Comments
              </a>
            </li>
            <li className="-mb-px mr-2 last:mr-0 flex-auto text-center">
